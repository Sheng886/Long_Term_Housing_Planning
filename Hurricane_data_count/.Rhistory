# Define a custom function to classify coasts
classify_coast <- function(longitude, latitude) {
if (longitude < -85) {
return("Gulf of Mexico")
} else {
return("Atlantic Ocean")
}
}
# Apply the classification function to the map data
usa_map <- usa_map %>%
mutate(coast = ifelse(region == "florida" & long > -85, "Atlantic Ocean",
ifelse(long < -85 & lat < 30, "Gulf of Mexico", NA)))
# Plot the USA map with different coast colors
ggplot(data = usa_map, aes(x = long, y = lat, group = group)) +
geom_polygon(fill = "lightgray", color = "black") + # Draw the base map
geom_path(aes(color = coast), size = 1) + # Draw the coastline with different colors
scale_color_manual(values = c("Atlantic Ocean" = "blue", "Gulf of Mexico" = "green")) +
coord_fixed(1.3) + # Fix aspect ratio
labs(title = "USA Map with Different Coastline Colors", x = "Longitude", y = "Latitude") +
theme_minimal()
