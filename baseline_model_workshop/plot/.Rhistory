depprivation_compare = read.csv("deprivation.csv")
ggplot(depprivation_compare,aes(x = Factor,y=Number,colour=House, group=House))+
geom_line(aes(linetype=House),size=1.2) +
geom_point(color="black",size=3,color = "black") +
xlab("Deprivation Cost Factor") +
ylab("Number of units") +
scale_y_continuous(labels = scales::comma)+
theme(text = element_text(size = 15))+
scale_fill_grey(start = 0, end = .9)
ggsave("deprivation.png")
