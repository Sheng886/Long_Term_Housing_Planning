library(zoo)
library(dplyr)
library(ggplot2)
# Load the leaflet library
library(leaflet)
# Define longitude and latitude coordinates for nodes
nodes <- data.frame(
name = c("New York City", "Los Angeles", "Chicago", "San Francisco"),
lon = c(-74.0060, -118.2437, -87.6298, -122.4194),
lat = c(40.7128, 34.0522, 41.8781, 37.7749)
)
# Create a leaflet map
map <- leaflet() %>%
addTiles() %>%
addCircleMarkers(
data = nodes,
lng = ~lon,
lat = ~lat,
radius = 5,
color = "red",
popup = ~name
) %>%
addLegend("bottomright", colors = "red", labels = nodes$name, title = "Nodes")
